<html>

<head>
    <title>Demo Socket IO</title>
    <link rel="stylesheet" type="text/css" href="layout.css" />
    <script src="jquery.js"></script>
    <script src="socket.io/socket.io.js"></script>
    <script src="xuly.js"></script>

</head>

<body>
    <div id="wrapper">
        <div id="loginForm">
            <h3>What's your name?</h3>
            <input type="text" id="txtUsername" />
            <input type="button" id="btnRegister" value="Register" />
        </div>
        <div id="chatForm">
            <div id="left">
                <div id="boxTitle">Online Users</div>
                <div id="boxContent"></div>
            </div>
            <div id="right">
                <div id="HiUser">
                    Hello <span id="currentUser">Teo</span>
                    <input type="button" value="Logout" id="btnLogout">
                </div>

                <div id="listMessages">
                </div>
                <div id="typingNoti"></div>
                <input type="text" id="txtMessage">
                <input type="button" id="btnSendMessage" value="Send">
            </div>
        </div>
    </div>
</body>

</html>